datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id          String   @id
  email       String
  displayName String?
  photoURL    String?
  createdAt   DateTime
}

model Task {
  id        String   @id
  label     String
  done      Boolean
  imageKey  String?
  createdAt DateTime
}
