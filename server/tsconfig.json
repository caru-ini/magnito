{
  "compilerOptions": {
    "downlevelIteration": true,
    "esModuleInterop": true,
    "lib": ["dom"],
    "module": "esnext",
    "moduleResolution": "node",
    "isolatedModules": true,
    "baseUrl": ".",
    "rootDir": ".",
    "skipLibCheck": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "resolveJsonModule": true,
    "target": "es6",
    "types": ["vitest/importMeta"],
    "incremental": true
  }
}
